                        <button class="control-btn primary" id="nextCommand" onclick="nextCommand()" style="display: none;">NEXT COMMAND ‚Üí</button>
                    </div>
                </div>
                
                <!-- Mission Complete -->
                <div id="missionComplete" class="mission-session">
                    <div class="mission-complete">
                        <div class="complete-icon" id="completeIcon">üèÜ</div>
