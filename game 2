        function goToMainMenu() {
            document.getElementById('quiz-screen').classList.add('hidden');
            document.getElementById('results-screen').classList.add('hidden');
            document.getElementById('main-menu').classList.remove('hidden');
            resetGame();
        }

        // Shuffle function
        function shuffleArray(array) {
            const shuffled = [...array];
            for (let i = shuffled.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [shuffled[i], shuffled[j]] = [shuffled[j], shuffled[i]];
            }
            return shuffled;
        }

        // Shuffle options while keeping track of correct answer
        function shuffleQuestionOptions(question) {
            const correctAnswer = question.options[question.correct];
            const shuffledOptions = shuffleArray(question.options);
            const newCorrectIndex = shuffledOptions.indexOf(correctAnswer);
            
            return {
                ...question,
                options: shuffledOptions,
                correct: newCorrectIndex
            };
        }

        function startQuiz(category) {
            gameState.currentCategory = category;
            
            // Shuffle questions first
            const shuffledQuestions = shuffleArray(quizData[category].questions);
            
            // Then shuffle options for each question
            gameState.questions = shuffledQuestions.map(question => shuffleQuestionOptions(question));
            
            gameState.currentQuestionIndex = 0;
            gameState.score = 0;
            gameState.correctAnswers = 0;
            gameState.incorrectAnswers = 0;
            gameState.selectedAnswer = null;
            gameState.userAnswers = new Array(gameState.questions.length).fill(null);
            gameState.isReviewing = false;
            gameState.highestQuestionReached = 0;

            document.getElementById('main-menu').classList.add('hidden');
            document.getElementById('quiz-screen').classList.remove('hidden');
            document.getElementById('results-screen').classList.add('hidden');

            setupQuiz();
            displayQuestion();
        }

        function setupQuiz() {
            const categoryData = quizData[gameState.currentCategory];
            document.getElementById('category-title').textContent = categoryData.title;
            updateUI();
        }

        function displayQuestion() {
            const question = gameState.questions[gameState.currentQuestionIndex];
            const userAnswer = gameState.userAnswers[gameState.currentQuestionIndex];
            
            document.getElementById('question-emoji').textContent = question.emoji;
            document.getElementById('question-text').textContent = question.question;
            
            const optionsContainer = document.getElementById('options-container');
            optionsContainer.innerHTML = '';
            
            question.options.forEach((option, index) => {
                const button = document.createElement('button');
                button.className = 'option-btn p-6 rounded-xl text-left font-semibold text-white text-lg';
                button.id = `option-${index}`;
                
                // If reviewing and this question was answered, show the results
                if (gameState.isReviewing && userAnswer !== null) {
                    button.disabled = true;
                    if (index === question.correct) {
                        button.classList.add('option-correct');
                    } else if (index === userAnswer && userAnswer !== question.correct) {
                        button.classList.add('option-incorrect');
                    } else {
                        button.classList.add('opacity-30');
                    }
                } else if (!gameState.isReviewing) {
                    button.onclick = () => selectAnswer(index);
                }
                
                button.innerHTML = `
                    <div class="flex items-center">
                        <div class="w-12 h-12 bg-gradient-to-r from-cyan-500 to-purple-500 text-white rounded-full flex items-center justify-center font-bold mr-4 text-xl neon-glow">
                            ${String.fromCharCode(65 + index)}
                        </div>
                        <span class="font-mono">${option}</span>
                    </div>
                `;
                
                optionsContainer.appendChild(button);
            });
            
            updateNavigationButtons();
            document.getElementById('auto-advance-indicator').classList.add('hidden');
            gameState.selectedAnswer = userAnswer;
        }

        function selectAnswer(selectedIndex) {
            if (gameState.selectedAnswer !== null || gameState.isReviewing) return;
            
            gameState.selectedAnswer = selectedIndex;
            gameState.userAnswers[gameState.currentQuestionIndex] = selectedIndex;
            
            // Update highest question reached
            if (gameState.currentQuestionIndex > gameState.highestQuestionReached) {
                gameState.highestQuestionReached = gameState.currentQuestionIndex;
            }
            
            const question = gameState.questions[gameState.currentQuestionIndex];
            const isCorrect = selectedIndex === question.correct;
            
            // Disable all buttons and show results
            for (let i = 0; i < question.options.length; i++) {
                const button = document.getElementById(`option-${i}`);
                button.disabled = true;
                button.classList.remove('hover:border-cyan-400');
                
                if (i === question.correct) {
                    button.classList.add('option-correct');
                } else if (i === selectedIndex && !isCorrect) {
                    button.classList.add('option-incorrect');
                } else {
                    button.classList.add('opacity-30');
                }
            }
            
            // Update score and stats only if this is a new answer
            if (isCorrect) {
                gameState.score += 20;
                gameState.correctAnswers++;
            } else {
                gameState.incorrectAnswers++;
            }
            
            updateUI();
            updateNavigationButtons();
            
            // Show countdown and auto-advance
            document.getElementById('auto-advance-indicator').classList.remove('hidden');
            let countdown = 2;
            document.getElementById('countdown').textContent = countdown;
            
            const countdownInterval = setInterval(() => {
                countdown--;
                document.getElementById('countdown').textContent = countdown;
                
                if (countdown <= 0) {
                    clearInterval(countdownInterval);
                    document.getElementById('auto-advance-indicator').classList.add('hidden');
                    nextQuestion();
                }
            }, 1000);
        }

        function nextQuestion() {
            gameState.currentQuestionIndex++;
            gameState.isReviewing = false;
            
            if (gameState.currentQuestionIndex >= gameState.totalQuestions) {
                showResults();
            } else {
                displayQuestion();
                updateUI();
            }
        }

        function goToPreviousQuestion() {
            if (gameState.currentQuestionIndex > 0) {
                gameState.currentQuestionIndex--;
                gameState.isReviewing = true;
                displayQuestion();
                updateUI();
            }
        }

        function goToCurrentQuestion() {
            gameState.currentQuestionIndex = gameState.highestQuestionReached + 1;
            gameState.isReviewing = false;
            
            if (gameState.currentQuestionIndex >= gameState.totalQuestions) {
                showResults();
            } else {
                displayQuestion();
                updateUI();
            }
        }

        function updateNavigationButtons() {
            const prevButton = document.getElementById('prev-button');
            const currentButton = document.getElementById('current-button');
            
            // Show previous button if not on first question
            if (gameState.currentQuestionIndex > 0) {
                prevButton.classList.remove('hidden');
            } else {
                prevButton.classList.add('hidden');
            }
            
            // Show current button if reviewing and not at current question
            if (gameState.isReviewing && gameState.currentQuestionIndex <= gameState.highestQuestionReached) {
                currentButton.classList.remove('hidden');
            } else {
                currentButton.classList.add('hidden');
            }
        }

        function updateUI() {
            // Update question counter
            document.getElementById('question-counter').textContent = 
                `LEVEL ${gameState.currentQuestionIndex + 1} / ${gameState.totalQuestions}`;
            
            // Update score
            document.getElementById('current-score').textContent = `SCORE: ${gameState.score}`;
            
            // Update circular progress
            const progress = ((gameState.currentQuestionIndex + 1) / gameState.totalQuestions) * 100;
            const circumference = 2 * Math.PI * 56;
            const offset = circumference - (progress / 100) * circumference;
            
            document.getElementById('progress-circle').style.strokeDashoffset = offset;
            document.getElementById('progress-text').textContent = `${Math.round(progress)}%`;
        }

        function showResults() {
            document.getElementById('quiz-screen').classList.add('hidden');
            document.getElementById('results-screen').classList.remove('hidden');
            
            const percentage = Math.round((gameState.correctAnswers / gameState.totalQuestions) * 100);
            
            // Set result emoji, title and grade based on performance
            let resultEmoji = "🎉";
            let resultTitle = "MISSION COMPLETE!";
            let grade = "B";
            let performanceMessage = "";
            
            if (percentage >= 90) {
                resultEmoji = "🏆";
                resultTitle = "LEGENDARY!";
                grade = "S+";
                performanceMessage = "PERFECT EXECUTION! YOU'RE A DIGITAL LEGEND! 🌟";
            } else if (percentage >= 80) {
                resultEmoji = "⚡";
                resultTitle = "EXCELLENT!";
                grade = "A";
                performanceMessage = "OUTSTANDING PERFORMANCE! NEON MASTER! 🎯";
            } else if (percentage >= 70) {
                resultEmoji = "🔥";
                resultTitle = "GREAT JOB!";
                grade = "B";
                performanceMessage = "SOLID PERFORMANCE! KEEP GLOWING! 💪";
            } else if (percentage >= 60) {
                resultEmoji = "⭐";
                resultTitle = "GOOD EFFORT!";
                grade = "C";
                performanceMessage = "DECENT RUN! UPGRADE YOUR SKILLS! 📚";
            } else {
                resultEmoji = "🚀";
                resultTitle = "KEEP TRAINING!";
                grade = "D";
                performanceMessage = "SYSTEM NEEDS UPGRADE! TRY AGAIN! 🔄";
            }
            
            document.getElementById('result-emoji').textContent = resultEmoji;
            document.getElementById('result-title').textContent = resultTitle;
            document.getElementById('final-score').textContent = gameState.score;
            document.getElementById('percentage-score').textContent = `${percentage}%`;
            document.getElementById('rank-display').textContent = grade;
            document.getElementById('performance-message').textContent = performanceMessage;
            document.getElementById('correct-count').textContent = gameState.correctAnswers;
            document.getElementById('incorrect-count').textContent = gameState.incorrectAnswers;
        }

        function restartQuiz() {
            startQuiz(gameState.currentCategory);
        }

        function resetGame() {
            gameState = {
                currentCategory: null,
                currentQuestionIndex: 0,
                score: 0,
                correctAnswers: 0,
                incorrectAnswers: 0,
                totalQuestions: 20,
                selectedAnswer: null,
                questions: [],
                userAnswers: [],
                isReviewing: false,
                highestQuestionReached: 0
            };
        }
    </script>
