                    question: "Ano ang chemical symbol ng ginto?",
                    answers: ["Go", "Gd", "Au", "Ag"],
                    correct: 2
                }
            ]
        };

        let currentCategory = '';
        let currentQuestionIndex = 0;
        let score = 0;
        let answered = false;
        let autoNextTimer = null;
        let timeLeft = 5;
        let userAnswers = []; // Store user answers for each question

        function shuffleArray(array) {
            const shuffled = [...array];
            for (let i = shuffled.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [shuffled[i], shuffled[j]] = [shuffled[j], shuffled[i]];
            }
            return shuffled;
        }

        function shuffleAnswers(question) {
            const originalAnswers = [...question.answers];
            const correctAnswer = originalAnswers[question.correct];
            
            // Create array of answer objects with original indices
            const answerObjects = originalAnswers.map((answer, index) => ({
                text: answer,
                isCorrect: index === question.correct
            }));
            
            // Shuffle the answers
            const shuffledAnswers = shuffleArray(answerObjects);
            
            // Find new correct index
            const newCorrectIndex = shuffledAnswers.findIndex(answer => answer.isCorrect);
            
            return {
                answers: shuffledAnswers.map(answer => answer.text),
                correct: newCorrectIndex
            };
        }

        function startQuiz(category) {
            currentCategory = category;
            currentQuestionIndex = 0;
            score = 0;
            answered = false;
            userAnswers = []; // Reset answer history
            
            // Shuffle questions and their answers
            const originalQuestions = quizData[category];
            const shuffledQuestions = shuffleArray(originalQuestions);
            
            // Shuffle answers for each question and update correct indices
            shuffledQuestions.forEach(question => {
                const shuffledData = shuffleAnswers(question);
                question.answers = shuffledData.answers;
                question.correct = shuffledData.correct;
            });
            
            // Store shuffled questions for this quiz session
            quizData[category + '_shuffled'] = shuffledQuestions;
            currentCategory = category + '_shuffled';
            
            document.getElementById('categorySelection').style.display = 'none';
            document.getElementById('quizSection').classList.add('active');
            
            showQuestion();
        }

        function showQuestion() {
            const questions = quizData[currentCategory];
            const question = questions[currentQuestionIndex];
            
            document.getElementById('question').textContent = question.question;
            
            const answersContainer = document.getElementById('answers');
            answersContainer.innerHTML = '';
            
            question.answers.forEach((answer, index) => {
                const button = document.createElement('button');
                button.className = 'answer-btn';
                button.textContent = answer;
                
                // Check if this question was already answered
                const previousAnswer = userAnswers[currentQuestionIndex];
                if (previousAnswer !== undefined) {
                    // Question was already answered, show the results
                    if (index === question.correct) {
                        button.classList.add('correct');
                    } else if (index === previousAnswer && index !== question.correct) {
                        button.classList.add('incorrect');
                    }
                    button.style.pointerEvents = 'none';
                } else {
                    // Question not answered yet, allow clicking
                    button.onclick = () => selectAnswer(index);
                }
                
                answersContainer.appendChild(button);
            });
            
            // Show/hide buttons based on whether question was answered
            const wasAnswered = userAnswers[currentQuestionIndex] !== undefined;
            if (wasAnswered) {
                document.getElementById('nextBtn').style.display = 'block';
                document.getElementById('timerDisplay').style.display = 'none';
                answered = true;
            } else {
                document.getElementById('nextBtn').style.display = 'none';
                document.getElementById('timerDisplay').style.display = 'none';
                answered = false;
            }
            
            // Update progress bar
            const progress = ((currentQuestionIndex + 1) / questions.length) * 100;
            document.getElementById('progressBar').style.width = progress + '%';
            
            // Update level indicator
            document.getElementById('levelIndicator').textContent = `LEVEL ${currentQuestionIndex + 1} / ${questions.length}`;
            
            // Update percentage indicator
            updatePercentageIndicator();
            
            // Update previous button
            const prevBtn = document.getElementById('prevBtn');
            if (currentQuestionIndex === 0) {
                prevBtn.disabled = true;
                prevBtn.style.opacity = '0.3';
            } else {
                prevBtn.disabled = false;
                prevBtn.style.opacity = '1';
            }
        }

        function selectAnswer(selectedIndex) {
            if (answered) return;
            
            answered = true;
            const questions = quizData[currentCategory];
            const question = questions[currentQuestionIndex];
            const buttons = document.querySelectorAll('.answer-btn');
            
            // Save the user's answer
            userAnswers[currentQuestionIndex] = selectedIndex;
            
            buttons.forEach((button, index) => {
                if (index === question.correct) {
                    button.classList.add('correct');
                } else if (index === selectedIndex && index !== question.correct) {
                    button.classList.add('incorrect');
                }
                button.style.pointerEvents = 'none';
            });
            
            if (selectedIndex === question.correct) {
                score += 10;
            }
            
            // Update percentage after answering
            updatePercentageIndicator();
            
            document.getElementById('nextBtn').style.display = 'block';
            document.getElementById('timerDisplay').style.display = 'block';
            
            // Start auto-next timer
            startAutoNextTimer();
        }
        
        function startAutoNextTimer() {
            timeLeft = 3;
            document.getElementById('timerDisplay').textContent = `AUTO NEXT IN: ${timeLeft}`;
            
            autoNextTimer = setInterval(() => {
                timeLeft--;
                document.getElementById('timerDisplay').textContent = `AUTO NEXT IN: ${timeLeft}`;
                
                if (timeLeft <= 0) {
                    clearInterval(autoNextTimer);
                    nextQuestion();
                }
            }, 1000);
        }
        
        function clearAutoNextTimer() {
            if (autoNextTimer) {
                clearInterval(autoNextTimer);
                autoNextTimer = null;
            }
        }
        
        function updatePercentageIndicator() {
            const answeredQuestions = userAnswers.filter(answer => answer !== undefined).length;
            
            if (answeredQuestions === 0) {
                document.getElementById('percentageIndicator').textContent = 'ACCURACY: 0%';
                return;
            }
            
            const questions = quizData[currentCategory];
            let correctAnswers = 0;
            
            userAnswers.forEach((userAnswer, index) => {
                if (userAnswer !== undefined && userAnswer === questions[index].correct) {
                    correctAnswers++;
                }
            });
            
            const percentage = Math.round((correctAnswers / answeredQuestions) * 100);
            document.getElementById('percentageIndicator').textContent = `ACCURACY: ${percentage}%`;
        }
        
        function previousQuestion() {
            if (currentQuestionIndex > 0) {
                clearAutoNextTimer();
                currentQuestionIndex--;
                showQuestion();
            }
        }

        function nextQuestion() {
            clearAutoNextTimer();
            currentQuestionIndex++;
            const questions = quizData[currentCategory];
            
            if (currentQuestionIndex < questions.length) {
                showQuestion();
            } else {
                showResults();
            }
        }

        function showResults() {
            document.getElementById('quizSection').classList.remove('active');
            document.getElementById('resultsSection').classList.add('active');
            
            const totalQuestions = quizData[currentCategory].length;
            const correctAnswers = score / 10; // Since each correct answer gives 10 points
            const percentage = Math.round((correctAnswers / totalQuestions) * 100);
            
            document.getElementById('finalScore').textContent = `CORRECT: ${correctAnswers}/${totalQuestions}`;
            document.getElementById('finalAccuracy').textContent = `ACCURACY: ${percentage}%`;
            
            let message = '';
            if (percentage >= 80) {
                message = '🏆 LEGENDARY PERFORMANCE! CYBER MASTER ACHIEVED!';
            } else if (percentage >= 60) {
                message = '🌟 EXCELLENT EXECUTION! MISSION SUCCESS!';
            } else if (percentage >= 40) {
                message = '👍 GOOD ATTEMPT! UPGRADE YOUR SKILLS!';
            } else {
                message = '📖 SYSTEM ERROR! REBOOT AND TRY AGAIN!';
            }
            
            document.getElementById('resultMessage').textContent = message;
        }

        function backToCategories() {
            clearAutoNextTimer();
            document.getElementById('quizSection').classList.remove('active');
            document.getElementById('categorySelection').style.display = 'grid';
        }

        function restartGame() {
            document.getElementById('resultsSection').classList.remove('active');
            document.getElementById('categorySelection').style.display = 'grid';
            score = 0;
        }
    </script>
